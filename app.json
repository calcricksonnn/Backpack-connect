{
  "expo": {
    "name": "Backpacker Connect",
    "slug": "backpacker-connect",
    "version": "1.0.0",
    "scheme": "backpackerconnect",
    "platforms": ["ios", "android"],
    "icon": "./assets/icon.png",
    "splash": { "image": "./assets/splash.png", "backgroundColor": "#ffffff" },
    "ios": {
      "bundleIdentifier": "com.yourcompany.backpackerconnect",
      "config": { "googleMapsApiKey": "YOUR_IOS_GOOGLE_MAPS_KEY" }
    },
    "android": {
      "package": "com.yourcompany.backpackerconnect",
      "config": { "googleMaps": { "apiKey": "YOUR_ANDROID_GOOGLE_MAPS_KEY" } }
    },
    "extra": {
      "eas": { "projectId": "YOUR_EAS_PROJECT_ID" },
      "firebase": {
        "apiKey": "YOUR_API_KEY",
        "authDomain": "YOUR_AUTH_DOMAIN",
        "projectId": "YOUR_PROJECT_ID",
        "storageBucket": "YOUR_BUCKET",
        "messagingSenderId": "YOUR_SENDER_ID",
        "appId": "YOUR_APP_ID"
      }
    }
  }
}