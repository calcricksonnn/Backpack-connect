# .env.example

# App Metadata
APP_NAME=Backpack Connect
APP_SLUG=backpack-connect
APP_VERSION=1.0.0
EXPO_SCHEME=backpackconnect

# Expo Updates (EAS)
EAS_UPDATE_URL=your-eas-update-url
EAS_PROJECT_ID=your-eas-project-id

# Bundle Identifiers
IOS_BUNDLE_IDENTIFIER=com.yourcompany.backpackconnect
ANDROID_PACKAGE=com.yourcompany.backpackconnect

# Firebase Config
FIREBASE_API_KEY=your-firebase-api-key
